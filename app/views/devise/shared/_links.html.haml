- if controller_name != 'sessions'
  = link_to "Logowanie", new_session_path(resource_name)
  %br

- if devise_mapping.registerable? && controller_name != 'registrations'
  = link_to "Rejestracja", new_registration_path(resource_name)
  %br

- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations'
  = link_to "Zgubione hasło?", new_password_path(resource_name)
  %br

- if devise_mapping.confirmable? && controller_name != 'confirmations'
  = link_to "Nie otrzymałeś/aś potwierdzenia?", new_confirmation_path(resource_name)
  %br

- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks'
  = link_to "Nie otrzymałeś/aś informacji dot. odblokowania?", new_unlock_path(resource_name)
  %br

- if devise_mapping.omniauthable?
  - resource_class.omniauth_providers.each do |provider|
    = link_to "Zaloguj się z #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider)
